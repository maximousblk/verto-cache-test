{
"rewrites": [
    { "source": "/", "destination": "https://v2-cache-verto-exchange.herokuapp.com/" }
  ],
  "headers": [
    {
      "source": "/token/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=60, stale-while-revalidate"
        }
      ]
    }
  ]
}
