{
"rewrites": [
    { "source": "/(.*)", "destination": "https://v2-cache-verto-exchange.herokuapp.com" }
  ],
  "headers": [
    {
      "source": "/token/(.*)/price",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=10, s-maxage=10, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=90, s-maxage=90, stale-while-revalidate"
        }
      ]
    }
  ]
}
