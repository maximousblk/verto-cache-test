{
"rewrites": [
    { "source": "/(.*)", "destination": "https://v2-cache-verto-exchange.herokuapp.com" }
  ],
  "headers": [
    {
      "source": "/token/(.*)/price",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "max-age=60, s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ]
}
